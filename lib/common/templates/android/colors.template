<% var props = _.filter(allProperties, function(prop) {
  return prop.attributes.category === 'color';
}); %>
<resources>
  <% _.each(props, function(prop) {
  %><color name="<%= prop.name %>"><%= prop.value %></color><% if (prop.comment) { %><!-- <%= prop.comment %> --><% } %>
  <% }); %>
</resources>
